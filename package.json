{
  "name": "https://github.com/aminariana/leetcode",
  "version": "1.0.0",
  "description": "LeetCode problems and solutions",
  "main": "index.js",
  "devDependencies": {
    "mocha": "^10.3.0"
  },
  "scripts": {
    "test": "mocha ./**/*.test.js",
    "list": "find . -maxdepth 1 -type d -regex '\\.\/[A-Z].*' | sed 's\/\\.\\\/\/\/' | sort",
    "count": "npm run list | wc -l",
    "list_java": "find . -maxdepth 2 -type d -name java | awk -F'/' '{print $2}' | grep -v 'Common' | sort",
    "list_python": "find . -maxdepth 2 -type d -name python | awk -F'/' '{print $2}' | grep -v 'Common' | sort",
    "count_java": "npm run list_java | wc -l",
    "count_python": "npm run list_python | wc -l"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aminariana/leetcode.git"
  },
  "author": "Amin Ariana",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aminariana/leetcode/issues"
  },
  "homepage": "https://github.com/aminariana/leetcode#readme"
}
